{ We are aiming for this SIMPLE structure: 
<xml>
<volume num="["0"-"9"]">

<chapter num="["0"-"9"]+" page="["0"-"9"]+">
   <stuff>....</stuff>
</chapter>
</xml>      
}

volume: -"SBS Volume ", vnum, newline+, chapter+.
@vnum: digit+.
chapter: -"Chapter ", num, page, newline+, textStream++newline.
@num: digit+, -comma, -space.
@page: -"Page ", digit+.
-newline: #d?,#a.
-digit: ["0"-"9"].
-comma: ",".
-space: " ".
-textStream: ["A"-"Z"], ~[#d;#a]+, newline?.